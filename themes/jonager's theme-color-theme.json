	"tokenColors": [
		{
			"name": "Keyword, Storage",
			"scope": [
        "keyword",
        "keyword.operator.assignment",
        "keyword.operator.new",
        "storage",
        "entity.name.label"
			],
			"settings": {
				"foreground": "#c678dd"
			}
		},
		{
			"name": "Variable",
			"scope": [
        "variable",
        "entity",
        "entity.name.type.class",
        "entity.name.type.interface",
        "entity.name.type.module",
        "meta",
        "meta.block.cpp",
        "support",
        "keyword.other.unit",
        "source",
			],
			"settings": {
				"foreground": "#cdd2db"
			}
    },
		{
			"name": "Meta Brace, Block",
			"scope": [
        "meta.brace",
        "meta.block",
        "punctuation"
			],
			"settings": {
				"foreground": "#447e81"
			}
    },
		{
			"name": "Support Primitive",
			"scope": [
        "support.type.primitive"
			],
			"settings": {
				"foreground": "#98C379"
			}
    },
		{
			"name": "Entity Function",
			"scope": [
        "entity.name.function",
        "entity.name.function-call",
        "support.function"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
    },
		{
			"name": "Entity Type",
			"scope": [
        "entity.name.type"
			],
			"settings": {
				"foreground": "#FFCB8B"
			}
    },
		{
			"name": "Entity HTML Tag",
			"scope": [
        "entity.name.tag",
        "support.class.component"
			],
			"settings": {
				"foreground": "#eebf67"
			}
    },
		{
			"name": "Entity Selector Style",
			"scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#56b6c2"
			}
    },
		{
			"name": "Variable Property",
			"scope": [
        "variable.other.property",
        "variable.other.env",
        "variable.other.constant.property",
        "meta.object-literal.key",
        "support.type.object.module",
        "support.type.property-name.json"
			],
			"settings": {
				"foreground": "#7FDBCA"
			}
    },
    {
			"name": "Comment",
			"scope": [
        "comment",
        "punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#386566"
			}
		},
    {
			"name": "String",
			"scope": [
        "string",
        "punctuation.definition.string"
			],
			"settings": {
				"foreground": "#98c379"
			}
		},
    {
			"name": "Constant",
			"scope": [
        "constant",
        "invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#F78C6C"
			}
		},
    {
			"name": "Constant Export-Import",
			"scope": [
        "constant.language.import-export-all"
			],
			"settings": {
				"foreground": "#bbc1cc"
			}
		}
	]
}